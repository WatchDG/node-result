{
  "name": "node-result",
  "version": "8.0.0",
  "description": "result",
  "keywords": [
    "result"
  ],
  "license": "MIT",
  "author": {
    "name": "Grigoriy Baranov",
    "email": "baranov@w3m.co"
  },
  "repository": "github:WatchDG/node-result",
  "private": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "fmt": "npx prettier --write \"src/**/*.ts\"",
    "lint": "npx eslint --ext=ts src",
    "test": "npx jest",
    "build": "npx tsc",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "prepare": "npm run lint && npm run build && npm run test",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "node-result-error": "^1.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "eslint": "^7.17.0",
    "husky": "^5.0.6",
    "jest": "^26.6.3",
    "pinst": "^2.1.1",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
